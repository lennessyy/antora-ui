{{!-- This is the navigation menu for one component, e.g. TigerGraph DB --}}
{{#with page.navigation}} {{!-- page.navigation accesses the nav.adoc file --}}
<div class="nav-panel-menu is-active" data-panel="menu">
  <nav class="nav-menu">
    {{#with @root.page.componentVersion}}
    <h3 class="title"><a href="{{{relativize ./url}}}">{{./title}} {{./displayVersion}}</a></h3>
    {{/with}}
{{> nav-tree navigation=this}}
  </nav>
</div>
{{/with}}
